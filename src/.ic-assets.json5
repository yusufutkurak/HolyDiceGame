{
  "defaults": {
    "headers": {
      "Content-Security-Policy": "default-src 'self'; script-src 'self' https://icp0.io https://*.icp0.io https://icp-api.io; connect-src 'self' https://icp-api.io wss://gateway.icws.io; img-src 'self' data: https://icp0.io; style-src 'self' 'unsafe-inline'; style-src-elem 'self' 'unsafe-inline'; font-src 'self' https://fonts.gstatic.com; object-src 'none'; base-uri 'self'; frame-ancestors 'none'; form-action 'self'; upgrade-insecure-requests;"
    }
  },
  "overrides": [
    {
      "pattern": "**/*",
      "security_policy": "standard"
    }
  ]
}
